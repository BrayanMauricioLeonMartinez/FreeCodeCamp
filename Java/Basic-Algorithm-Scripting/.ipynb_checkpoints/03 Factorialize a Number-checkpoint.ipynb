    "    num = 0\n",
    "    def __init__(self, num):\n",
    "        self.num = 0\n",
    "    \n",
    "    def factorizide(self, factor):\n",
    "        if(factor == 1):\n",
    "            return 1\n",
    "        return factor * self.factorizide(factor -1)\n",
    "\n",
    "result = mat(0)\n",
    "print(result.factorizide(5))"
